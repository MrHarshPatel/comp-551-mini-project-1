   "source": [
    "def evaluate_acc(y, yh):\n",
    "    tp = np.sum(np.logical_and(yh==1, y==1))\n",
    "    tn = np.sum(np.logical_and(yh==0, y==0))\n",
    "    \n",
    "    acc = (tp + tn)/y.size\n",
    "    return acc"
   ]
